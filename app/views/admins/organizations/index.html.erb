<p id="notice"><%= notice %></p>

<h1>Admins Organizations</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Encrypted password</th>
      <th>Tax</th>
      <th>Email</th>
      <th>Name</th>
      <th>Address</th>
      <th>Created by</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admins_organizations.each do |admins_organization| %>
      <tr>
        <td><%= admins_organization.username %></td>
        <td><%= admins_organization.encrypted_password %></td>
        <td><%= admins_organization.tax_id %></td>
        <td><%= admins_organization.email %></td>
        <td><%= admins_organization.name %></td>
        <td><%= admins_organization.address %></td>
        <td><%= admins_organization.created_by %></td>
        <td><%= link_to 'Show', admins_organization %></td>
        <td><%= link_to 'Edit', edit_admins_organization_path(admins_organization) %></td>
        <td><%= link_to 'Destroy', admins_organization, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admins Organization', new_admins_organization_path %>
